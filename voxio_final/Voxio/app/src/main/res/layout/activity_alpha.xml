<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.pooppai.joycy.myproject.AlphaActivity"
    android:background="#c1cbf0"
    android:orientation="vertical">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="60dp">

        <ImageView
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_marginLeft="40dp"
            android:layout_gravity="center"
            android:id="@+id/imageView5"
            android:background="@drawable/voxio" />


    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:id="@+id/imgll">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="180dp"
            android:id="@+id/imageView"
            android:background="#a1b1eb"
            android:onClick="openPic"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="10dp"
        android:gravity="center">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal">

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonA"
                android:background="@drawable/a"
                android:onClick="openA"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonB"
                android:background="@drawable/b"
                android:onClick="openB"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonC"
                android:background="@drawable/c"
                android:onClick="openC"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonD"
                android:background="@drawable/d"
                android:onClick="openD"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonE"
                android:background="@drawable/e"
                android:onClick="openE"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonF"
                android:background="@drawable/f"
                android:onClick="openF"/>


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal">

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonG"
                android:background="@drawable/g"
                android:onClick="openG"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonH"
                android:background="@drawable/h"
                android:onClick="openH"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonI"
                android:background="@drawable/i"
                android:onClick="openI"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonJ"
                android:background="@drawable/j"
                android:onClick="openJ"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonK"
                android:background="@drawable/k"
                android:onClick="openK"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonL"
                android:background="@drawable/l"
                android:onClick="openL"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal">

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonM"
                android:background="@drawable/m"
                android:onClick="openM"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonN"
                android:background="@drawable/n"
                android:onClick="openN"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonO"
                android:background="@drawable/o"
                android:onClick="openO"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonP"
                android:background="@drawable/p"
                android:onClick="openP"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonQ"
                android:background="@drawable/q"
                android:onClick="openQ"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonR"
                android:background="@drawable/r"
                android:onClick="openR"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal">

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonS"
                android:background="@drawable/s"
                android:onClick="openS"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonT"
                android:background="@drawable/t"
                android:onClick="openT"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonU"
                android:background="@drawable/u"
                android:onClick="openU"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonV"
                android:background="@drawable/v"
                android:onClick="openV"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonW"
                android:background="@drawable/w"
                android:onClick="openW"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonX"
                android:background="@drawable/x"
                android:onClick="openX"/>

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal">

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginLeft="93dp"
                android:id="@+id/imageButtonY"
                android:background="@drawable/y"
                android:onClick="openY"/>

            <ImageButton
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:id="@+id/imageButtonZ"
                android:background="@drawable/z"
                android:onClick="openZ"/>

        </LinearLayout>
    </LinearLayout>

</LinearLayout>